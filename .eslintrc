{
  "extends": ["taro"],
  "rules": {
    "no-unused-vars": ["error", {
      "varsIgnorePattern": "Taro"
    }],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx", ".tsx"]
    }],
    "space-before-function-paren": "off",
    "array-bracket-spacing": "off",
    "jsx-quotes": "off"
  },
  "parser": "babel-eslint"
}
